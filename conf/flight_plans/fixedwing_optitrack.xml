<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="2" ground_alt="0" lat0="51.989180" lon0="4.376199" max_dist_from_home="10" name="Fixed Wing Optitrack OJF (Delft)" security_height="0.3">
  <header>
#include "autopilot.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint lat="51.9891798" lon="4.3761986" name="TD"/>
    <waypoint lat="51.9891747" lon="4.3761986" name="CLIMB"/>
    <waypoint lat="51.9891711" lon="4.3761985" name="STDBY"/>
    <waypoint lat="51.989240" lon="4.376201" name="p1"/>
    <waypoint lat="51.9892652" lon="4.3762012" name="p2"/>
    <waypoint lat="51.9891950" lon="4.3762231" name="FA1"/>
    <waypoint lat="51.9891557" lon="4.3762221" name="FA2"/>
    <waypoint lat="51.9891563" lon="4.3761772" name="FA3"/>
    <waypoint lat="51.9891957" lon="4.3761770" name="FA4"/>
  </waypoints>
  <sectors>
    <sector color="red" name="Flight_Area">
      <corner name="FA4"/>
      <corner name="FA3"/>
      <corner name="FA2"/>
      <corner name="FA1"/>
    </sector>
  </sectors>
  <blocks>
    <block name="Wait GPS">
      <set value="1" var="kill_throttle"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Holding point">
      <set value="1" var="kill_throttle"/>
      <attitude roll="0" throttle="0" vmode="throttle"/>
    </block>
    <block group="home" key="t" name="Takeoff" strip_button="Takeoff (wp CLIMB)" strip_icon="takeoff.png">
      <exception cond="GetPosAlt() > GetAltRef()+25" deroute="Standby"/>
      <set value="0" var="kill_throttle"/>
      <set value="0" var="autopilot_flight_time"/>
      <go from="HOME" pitch="15" throttle="1.0" vmode="throttle" wp="CLIMB"/>
    </block>
    <block group="home" key="Ctrl+a" name="Standby" strip_button="Standby" strip_icon="home.png">
      <go from="CLIMB" hmode="route" wp="STDBY"/>
    </block>
    <block name="Go to wp1">
      <go wp="p1"/>
      <deroute block="Go to wp1"/>
    </block>
    <block name="flare">
      <go approaching_time="0" from="STDBY" hmode="route" throttle="0.0" vmode="throttle" wp="TD"/>
      <attitude roll="0.0" throttle="0.0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
