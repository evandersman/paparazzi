<!DOCTYPE airframe SYSTEM "../airframe.dtd">

<!-- Edge540 HobbyKing
     Lisa/M v2.0 board (http://wiki.paparazziuav.org/wiki/Lisa/M_v2)
     Aspirin IMU v2.1 with MS5611 barometer
     Xbee modem in xbee api mode
-->

<airframe name="Slick360 Lisa/M 2.0">

  <firmware name="fixedwing">
    <target name="ap" board="lisa_m_2.0">
      <define name="LISA_M_OR_MX_21"/>
      <configure name="FLASH_MODE" value="SWD"/>
      <configure name="PERIODIC_FREQUENCY" value="512"/>
      <configure name="AHRS_PROPAGATE_FREQUENCY" value="512"/>
      <configure name="AHRS_CORRECT_FREQUENCY" value="512"/>
    </target>
    <target name="sim" board="pc"/>
    <target name="nps" board="pc">
      <subsystem name="fdm" type="jsbsim"/>
    </target>

    <configure name="USE_MAGNETOMETER" value="FALSE"/>

    <define name="STRONG_WIND"/>

    <subsystem name="radio_control" type="spektrum">
      <!--define name="RADIO_KILL_SWITCH" value="RADIO_AUX2"/-->
      <define name="RADIO_MODE" value="RADIO_GEAR"/>
    </subsystem>
    <subsystem name="actuators" type="pwm">
      <define name="SERVO_HZ" value="400"/>
    </subsystem>

    <!-- Communication -->
    <!-- WiFi module -->
    <subsystem name="telemetry" type="transparent">
      <configure name="MODEM_BAUD" value="B115200"/>
    </subsystem>
    <!-- XBee -->
    <!--subsystem name="telemetry" type="xbee_api"/-->

    <!-- Sensors -->
    <subsystem name="imu" type="aspirin_v2.1"/>
    <!-- for optitrack with Wifi module -->
    <module name="gps" type="optitrack"/>
    <!-- for outdoor flight -->
    <!--subsystem name="gps" type="ublox"/-->

    <subsystem name="ahrs" type="int_cmpl_quat">
      <define name="USE_GPS" value="1"/>
      <define name="AHRS_USE_GPS_HEADING" value="1"/>
      <define name="AHRS_GRAVITY_UPDATE_COORDINATED_TURN" value="1"/>
    </subsystem>
    <subsystem name="control" type="indi"/>
    <subsystem name="navigation"/>
    <module name="ins" type="alt_float">
    </module>
  </firmware>

  <modules main_freq="512">
    <!--load name="turbulence_adc.xml"/-->
    <!--load name="potentiometer_adc.xml"/-->
    <load name="logger_spi_link.xml"/>
    <load name="nav_throttle_wt.xml">
      <define name="H_CTL_THROTTLE_PGAIN" value="0.01"/>
      <define name="H_CTL_THROTTLE_IGAIN" value="0."/>
      <define name="H_CTL_THROTTLE_DGAIN" value="0."/>
    </load>
    <!--load name="gps_ubx_ucenter.xml"/-->
  </modules>

  <servos>
    <servo name="THROTTLE" no="0" min="1100" neutral="1100" max="1900"/>
    <servo name="AILERON_LEFT" no="1" min="2000" neutral="1500" max="1000"/>
    <servo name="AILERON_RIGHT" no="2" min="2000" neutral="1500" max="1000"/>
    <servo name="ELEVATOR" no="3" min="2000" neutral="1500" max="1000"/>
    <servo name="RUDDER" no="4" min="2000" neutral="1500" max="1000"/>
  </servos>

  <commands>
    <axis name="THROTTLE" failsafe_value="0"/>
    <axis name="ROLL" failsafe_value="0"/>
    <axis name="PITCH" failsafe_value="0"/>
    <axis name="YAW" failsafe_value="0"/>
    <axis name="TURB_LEFT" failsafe_value="0"/>
    <axis name="TURB_RIGHT" failsafe_value="0"/>
  </commands>

  <rc_commands>
    <set command="THROTTLE" value="@THROTTLE"/>
    <set command="ROLL" value="@ROLL"/>
    <set command="PITCH" value="@PITCH"/>
    <set command="YAW" value="@YAW"/>
  </rc_commands>

  <section name="MIXER">
    <define name="COMBI_SWITCH" value="0.3"/>
  </section>

  <command_laws>
    <set servo="THROTTLE" value="@THROTTLE"/>
    <set servo="ELEVATOR" value="@PITCH"/>
    <set servo="RUDDER" value="@YAW"/>
    <set servo="AILERON_LEFT" value="@ROLL"/>
    <set servo="AILERON_RIGHT" value="@ROLL"/>
    <!--with turbulence feedforward-->
    <!--set servo="AILERON_LEFT" value="@ROLL + @TURB_LEFT"/>
    <set servo="AILERON_RIGHT" value="@ROLL - @TURB_RIGHT"/-->
  </command_laws>

  <section name="IMU" prefix="IMU_">
    <!-- ACCEL and GYRO calibration left out to take default datasheet values -->

    <!-- replace this with your own mag calibration -->
    <define name="MAG_X_NEUTRAL" value="-45"/>
    <define name="MAG_Y_NEUTRAL" value="334"/>
    <define name="MAG_Z_NEUTRAL" value="7"/>
    <define name="MAG_X_SENS" value="4.47647816128" integer="16"/>
    <define name="MAG_Y_SENS" value="4.71085671542" integer="16"/>
    <define name="MAG_Z_SENS" value="4.41585354498" integer="16"/>

    <define name="BODY_TO_IMU_PHI" value="0." unit="deg"/>
    <define name="BODY_TO_IMU_THETA" value="10." unit="deg"/>
    <define name="BODY_TO_IMU_PSI" value="0." unit="deg"/>
  </section>

  <section name="INS" prefix="INS_">
    <define name="ROLL_NEUTRAL_DEFAULT" value="0" unit="deg"/>
    <define name="PITCH_NEUTRAL_DEFAULT" value="0" unit="deg"/>
  </section>

  <section name="VERTICAL CONTROL" prefix="V_CTL_">
    <define name="POWER_CTL_BAT_NOMINAL" value="11.1" unit="volt"/>
    <!-- outer loop proportional gain -->
    <define name="ALTITUDE_PGAIN" value="0.2"/>
    <!-- outer loop saturation -->
    <define name="ALTITUDE_MAX_CLIMB" value="2."/>
    <define name="PITCH_TRIM" value="0." unit="deg"/>

    <!-- auto throttle inner loop -->
    <define name="AUTO_THROTTLE_NOMINAL_CRUISE_THROTTLE" value="0.52"/>
    <define name="AUTO_THROTTLE_MIN_CRUISE_THROTTLE" value="0.45"/>
    <define name="AUTO_THROTTLE_MAX_CRUISE_THROTTLE" value="0.65"/>
    <define name="AUTO_THROTTLE_CLIMB_THROTTLE_INCREMENT" value="0.04" unit="%/(m/s)"/>
    <define name="AUTO_THROTTLE_PGAIN" value="0.0"/>
    <define name="AUTO_THROTTLE_IGAIN" value="0.0"/>
    <define name="AUTO_THROTTLE_PITCH_OF_VZ_PGAIN" value="1."/>
  </section>

  <section name="HORIZONTAL CONTROL" prefix="H_CTL_">
    <define name="COURSE_PGAIN" value="1.0"/>
    <define name="COURSE_DGAIN" value="0.3"/>

    <define name="ROLL_MAX_SETPOINT" value="35" unit="deg"/>
    <define name="PITCH_MAX_SETPOINT" value="20" unit="deg"/>
    <define name="PITCH_MIN_SETPOINT" value="-10" unit="deg"/>

    <define name="PITCH_PGAIN" value="4500."/>
    <define name="PITCH_DGAIN" value="1.5"/>
    <define name="PITCH_IGAIN" value="1"/>

    <define name="ELEVATOR_OF_ROLL" value="1250"/>

    <define name="ROLL_SLEW" value="0.1"/>

    <define name="ROLL_ATTITUDE_GAIN" value="12500"/>
    <define name="ROLL_RATE_GAIN" value="700"/>
    <define name="ROLL_I_GAIN" value="1"/>
  </section>

  <section name="STABILIZATION_ATTITUDE_INDI" prefix="STABILIZATION_INDI_">
    <!-- control effectiveness -->
    <define name="G_ROLL" value="0.019"/>
    <define name="G_PITCH" value="0.013"/>

    <!-- reference acceleration for attitude control -->
    <define name="REF_ERR_P" value="420.0"/>
    <define name="REF_ERR_Q" value="420."/>
    <define name="REF_ERR_R" value="600.0"/>
    <define name="REF_RATE_P" value="20.0"/>
    <define name="REF_RATE_Q" value="20.0"/>
    <define name="REF_RATE_R" value="28.0"/>

    <!-- second order filter parameters -->
    <define name="FILT_OMEGA" value="20.0"/>
    <define name="FILT_ZETA" value="0.65"/>

    <!-- first order actuator dynamics -->
    <define name="ACT_DYN_P" value="0.35"/>
  </section>

  <section name="FAILSAFE" prefix="FAILSAFE_">
    <define name="DELAY_WITHOUT_GPS" value="2" unit="s"/>
    <define name="DEFAULT_THROTTLE" value="0.1" unit="%"/>
    <define name="DEFAULT_ROLL" value="0.3" unit="rad"/>
    <define name="DEFAULT_PITCH" value="0.5" unit="rad"/>
  </section>

  <section name="AUTO1" prefix="AUTO1_">
    <define name="MAX_ROLL" value="60." unit="deg"/>
    <define name="MAX_PITCH" value="80." unit="deg"/>
  </section>

  <section name="BAT">
    <define name="MILLIAMP_AT_FULL_THROTTLE" value="2000"/>
    <define name="CATASTROPHIC_BAT_LEVEL" value="9.4" unit="V"/>
    <define name="CRITIC_BAT_LEVEL" value="10.4" unit="V"/>
    <define name="LOW_BAT_LEVEL" value="10.8" unit="V"/>
    <define name="MAX_BAT_LEVEL" value="12.4" unit="V"/>
  </section>

  <section name="MISC">
    <define name="NOMINAL_AIRSPEED" value="10." unit="m/s"/>
    <define name="CARROT" value="5." unit="s"/>
    <define name="KILL_MODE_DISTANCE" value="(1.5*MAX_DIST_FROM_HOME)"/>
    <define name="CONTROL_FREQUENCY" value="512" unit="Hz"/>
    <define name="NAVIGATION_FREQUENCY" value="20" unit="Hz"/>
    <define name="DEFAULT_CIRCLE_RADIUS" value="40."/>  
    <define name="AIRSPEED_LEFT_OFFSET" value="30"/>
    <define name="PITCH_LEFT_OFFSET" value="24"/>
    <define name="AIRSPEED_RIGHT_OFFSET" value="30"/>
    <define name="PITCH_RIGHT_OFFSET" value="35"/>
    <define name="TURB_PGAIN" value="14000"/>
    <define name="TURB_ACC_GAIN" value="1"/>
    <define name="PITCH_OMEGA" value="0.5"/>
    <define name="PITCH_ZETA" value="1"/>
    <define name="SERVO_DELAY" value="6"/>
    <!--define name="PROBES_FF_ANG_ACC" value="1"/-->
    <define name="COMMAND_PITCH_TRIM" value="0."/>
    <define name="OUTBACK_CHALLENGE_DANGEROUS_RULE_RC_LOST_NO_AP" value="TRUE"/>
    <define name="STEP_INPUT_OPEN_LOOP_ROLL" value="FALSE"/>
    <define name="STEP_INPUT_OPEN_LOOP_PITCH" value="FALSE"/>
    <define name="STEP_INPUT_AUTO1_ROLL" value="FALSE"/>
    <define name="STEP_INPUT_AUTO1_PITCH" value="FALSE"/>
  </section>

  <section name="SIMULATOR" prefix="NPS_">
    <define name="JSBSIM_MODEL" value="&quot;easystar&quot;"/>
    <define name="JSBSIM_LAUNCHSPEED" value="20"/>
    <define name="SENSORS_PARAMS" value="&quot;nps_sensors_params_default.h&quot;"/>
    <define name="JS_AXIS_MODE" value="4"/>
  </section>

</airframe>
